<Window x:Class="WpfApp3_Bindings.Ex3Binding_DataContext"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3_Bindings"
        mc:Ignorable="d"
        Title="Ex3Binding_DataContext" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Text="{Binding EmpId}"></TextBox>

            <TextBox Text="{Binding EmpName}"></TextBox>
            <TextBox Text="{Binding Address}"></TextBox>
        </StackPanel>
        
    </Grid>
</Window>
